{
  "name": "jsdoc-template-argon",
  "description": "A JSDoc template.",
  "version": "0.1.0",
  "main": "publish.js",
  "scripts": {
    "lint": "eslint publish.js src/",
    "example:lodash": "jsdoc examples/lodash/lodash-repo -t . -d docs/example/lodash -R examples/lodash/README.md",
    "docs": "jsdoc . -t . -d docs -R docs-static/README.md"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Oza94/jsdoc-template-argon"
  },
  "bugs": {
    "url": "https://github.com/Oza94/jsdoc-template-argon/issues"
  },
  "keywords": [
    "jsdoc",
    "jsdoc3",
    "template"
  ],
  "dependencies": {
    "bluebird": "^3.5.0",
    "ejs": "^2.5.6",
    "fs-extra-promise": "^0.4.1",
    "slugify": "^1.1.0",
    "taffydb": "^2.7.3"
  },
  "license": "MIT",
  "devDependencies": {
    "eslint": "^3.18.0"
  }
}
